<machine>
  <states>
    <state>
      <name>j:Auction(auctionID(A))</name>
      <start>false</start>
      <end>true</end>
      <contains>
        <state>
          <name>b:BidderEnrollment(auctionID(A),enrollID(E))</name>
          <start>true</start>
          <end>false</end>
          <contains>
                <state>
                  <name>A1</name>
                  <start>true</start>
                  <end>false</end>
                  <contains></contains>
                  <outgoingTransitions>
                    <transition>from_A1_to_A2_label_r2</transition>
                    <transition>from_A1_to_A2_label_r7</transition>
                  </outgoingTransitions>
                </state>
                <state>
                  <name>A2:banana</name>
                  <start>false</start>
                  <end>true</end>
                  <contains></contains>
                  <outgoingTransitions>
                    <transition>from_A2_to_B</transition>
                    <transition>from_A2_to_E</transition>
                    <transition>from_A2_to_C1</transition>
                  </outgoingTransitions>
                </state>
                <state>
                  <name>X</name>
                  <start>false</start>
                  <end>false</end>
                  <contains>
                    <state>
                      <name>C1:Kitty</name>
                      <start>false</start>
                      <end>true</end>
                      <contains></contains>
                      <outgoingTransitions></outgoingTransitions>
                      </state>
                  </contains>
                  <outgoingTransitions></outgoingTransitions>
                  <ingoingTransitions>
           	    <transition>from_A2_to_C1</transition>
	  	    <transition>from_B_to_C1</transition>
         	  </ingoingTransitions>
                </state>
          </contains>
          <outgoingTransitions>
            <transition>from_Y_to_A1</transition>
	    <transition>from_A2_to_B</transition>
	    <transition>from_A2_to_E</transition>
          </outgoingTransitions>
	  <ingoingTransitions>
            <transition>from_Y_to_A1</transition>
	    <transition>from_nowhere_to_A1</transition>
            <transition>from_B_to_C1</transition>
          </ingoingTransitions>
        </state>
        <state>
          <name>i:ItemOffer(ItemID(i))</name>
          <start>false</start>
          <end>true</end>
          <contains>
            <state>
              <name>B</name>
              <start>false</start>
              <end>false</end>
              <contains></contains>
              <outgoingTransitions>
                <transition>from_B_to_E</transition>
                <transition>from_B_to_C1</transition>
              </outgoingTransitions>
            </state>
          </contains>
          <outgoingTransitions>
            <transition>from_B_to_E</transition>
            <transition>from_B_to_C1</transition>
          </outgoingTransitions>
          <ingoingTransitions>
            <transition>from_A2_to_B</transition>
          </ingoingTransitions>
        </state>
        <state>
          <name>E:Elve</name>
          <start>false</start>
          <end>true</end>
          <contains></contains>
          <outgoingTransitions>
            <transition></transition>
          </outgoingTransitions>
        </state>
      </contains>
      <outgoingTransitions></outgoingTransitions>
      <ingoingTransitions>
        <transition>from_nowhere_to_A1</transition>
      </ingoingTransitions>
    </state>
  </states>

  <transitions>
    <transition>
      <identifier>from_nowhere_to_A1</identifier>
      <source></source>
      <target>A1</target>
      <label>a:auctionBegin(auctionID(A*))</label> 
      <enter>true</enter>
      <instantiating>true</instantiating>
      <terminating>false</terminating>
      <container></container>
    </transition>
    <transition>
      <identifier>from_A1_to_A2_label_r2</identifier>
      <source>A1</source>
      <target>A2:banana</target>
      <label>auctionEnd(auctionID(A));20&gt;=this.end-j.end</label>
      <enter>false</enter>
      <instantiating>false</instantiating>
      <terminating>false</terminating>
      <container>b:BidderEnrollment(auctionID(A),enrollID(E))</container>
    </transition>
    <transition>
      <identifier>from_Y_to_A1</identifier>
      <source>b:BidderEnrollment(auctionID(A),enrollID(E))</source>
      <target>A1</target>
      <label>eBc:enrollmentBegin(auctionID(A),enrollID(E*));this.end-i.end&lt;20</label>
      <enter>false</enter>
      <instantiating>true</instantiating>
      <terminating>false</terminating>
      <container>j:Auction(auctionID(A))</container>
    </transition>
    <transition>
      <identifier>from_A2_to_B</identifier>
      <source>A2:banana</source>
      <target>B</target>
      <label>u:UHU;this.end%5&lt;=6</label>
      <enter>false</enter>
      <instantiating>true</instantiating>
      <terminating>true</terminating>
      <container>j:Auction(auctionID(A))</container>
    </transition>
    <transition>
      <identifier>from_A2_to_E</identifier>
      <source>A2:banana</source>
      <target>E:Elve</target>
      <label>iD:itemDescription(auctionID(A),itemID(I*),price(P*));30&lt;=this.end-j.end;b.countFinished!=2</label>
      <enter>false</enter>
      <instantiating>false</instantiating>
      <terminating>true</terminating>
      <container>j:Auction(auctionID(A))</container>
    </transition>
    <transition>
      <identifier>from_B_to_E</identifier>
      <source>B</source>
      <target>E:Elve</target>
      <label>bid</label>
      <enter>false</enter>
      <instantiating>false</instantiating>
      <terminating>true</terminating>
      <container>j:Auction(auctionID(A))</container>
    </transition>
    <transition>
      <identifier>from_B_to_C1</identifier>
      <source>B</source>
      <target>C1:Kitty</target>
      <label>sell;b.countFinished*2&gt;3;this.end/5=4</label>
      <enter>false</enter>
      <instantiating>true</instantiating>
      <terminating>true</terminating>
      <container>j:Auction(auctionID(A))</container>
    </transition>
    <transition>
      <identifier>from_A2_to_C1</identifier>
      <source>A2:banana</source>
      <target>C1:Kitty</target>
      <label>hammerBeat(itemID(I));this.end=10;f.end*2&lt;5</label>
      <enter>false</enter>
      <instantiating>true</instantiating>
      <terminating>false</terminating>
      <container>b:BidderEnrollment(auctionID(A),enrollID(E))</container>
    </transition>
    <transition>
      <identifier>from_A1_to_A2_label_r7</identifier>
      <source>A1</source>
      <target>A2:banana</target>
      <label>f:false(at1(A*),at2(B),at3(C*),at4(D));last.end*this.end&gt;9</label>
      <enter>false</enter>
      <instantiating>false</instantiating>
      <terminating>false</terminating>
      <container>b:BidderEnrollment(auctionID(A),enrollID(E))</container>
    </transition>
  </transitions>
</machine>